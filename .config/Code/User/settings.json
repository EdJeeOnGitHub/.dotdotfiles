{   "editor.guides.bracketPairs" : "active",
    "editor.hover.above": false,
    "vim.insertModeKeyBindings": [
        {
            "before": ["j", "j"],
            "after": ["<Esc>"]
        }
    ],
    "editor.bracketPairColorization.enabled": true,
    "editor.rulers": [80],
    "vim.foldfix": true,
    "latex-workshop.docker.image.latex": "blang/latex:ubuntu",
    "latex-workshop.docker.enabled": true,
    "latex-workshop.view.pdf.viewer": "tab",
    "workbench.colorTheme": "Gruvbox Dark (Hard)",
    "git.ignoreLegacyWarning": true,
    "window.menuBarVisibility": "toggle",
    "workbench.statusBar.visible": true,
    "breadcrumbs.enabled": true,
    "terminal.integrated.commandsToSkipShell": [
        "language-julia.interrupt"
    ],
    "julia.enableTelemetry": true,
    "editor.renderControlCharacters": true,
    "editor.fontLigatures": true,
    "editor.fontFamily": "'Fira Code', 'Droid Sans Mono', 'monospace', monospace, 'Droid Sans Fallback'",
    "terminal.integrated.fontFamily": " 'Droid Sans Mono', 'monospace', monospace, 'Droid Sans Fallback'",
    "terminal.external.linuxExec": "kitty",
    "editor.fontSize": 16,
    "r.sessionWatcher": true,
    "jupyter.interactiveWindowMode": "perFile",
    "jupyter.askForKernelRestart": false,
    "r.bracketedPaste": true,
    "r.rterm.linux": "/usr/bin/radian",
    "workbench.editorAssociations": {
        "*.ipynb": "jupyter.notebook.ipynb"
    },
    "vim.useSystemClipboard": true,
    "vva.trustedDomain": true,
    "editor.lineNumbers": "relative",
    "vim.enableNeovim": true,
    "vim.neovimPath": "/usr/bin/nvim",
    "julia.enableCrashReporter": false,
    "editor.inlineSuggest.enabled": true,
    "julia.execution.codeInREPL": true,
    "julia.symbolCacheDownload": true,
    "julia.additionalArgs": [
    "--threads=4"
    ],
    "python.defaultInterpreterPath": "/usr/local/bin/python",
    "github.copilot.enable": {
        "*": false,
        "yaml": false,
        "plaintext": true,
        "markdown": false
    },
    "editor.minimap.enabled": false,
    "julia.NumThreads": 4,
    "vim.camelCaseMotion.enable": true,
    "vim.neovimUseConfigFile": true,
    "terminal.integrated.enableMultiLinePasteWarning": false,
    "editor.acceptSuggestionOnEnter": "smart",
    "editor.hover.delay": 10,
    "r.lsp.diagnostics": false,
    "r.lsp.enabled": false,
    "window.zoomLevel": -3
}